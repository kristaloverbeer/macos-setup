alias la='ls --color=tty -lhA'
alias df='df -Tha --total'
alias du='du -ach | sort -h'
alias ps='ps axf'
alias psu='ps auxf'
alias psg='ps aux | grep -v grep | grep -i -e VSZ -e'
alias top='htop'
alias rmdocker='docker rm -vf $(docker ps -a -q)'
alias rmidocker='docker rmi -f $(docker images -q)'
alias notebook='docker run -it --rm -p 8888:8888 -v ~/workspace/:/home/jovyan/work jupyter/datascience-notebook'
alias delpyc='sudo find . \( -name __pycache__ -o -name "*.pyc" \) -delete'
alias delbranches='git branch -D $(git branch --merged | grep -v master)'
alias ggupdate='git checkout master && git fetch origin --prune && git pull origin master && git checkout - && git rebase master'
