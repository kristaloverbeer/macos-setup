alias la='ls --color=tty -lhA'
alias df='df -Tha --total'
alias du='du -ach | sort -h'
alias ps='ps axf'
alias psu='ps auxf'
alias psg='ps aux | grep -v grep | grep -i -e VSZ -e'
alias top='htop'
alias rmdocker='docker rm -vf $(docker ps -a -q)'
alias rmidocker='docker rmi -f $(docker images -q)'
alias postman='/home/emeric/applications/Postman/Postman'
alias touchpadon='xinput set-prop 12 "Synaptics Off" 0'
alias touchpadoff='xinput set-prop 12 "Synaptics Off" 1'
alias notebook='docker run -it --rm -p 8888:8888 -v ~/workspace/:/home/jovyan/work jupyter/datascience-notebook'
alias postman='/opt/Postman/Postman &'
alias franz='/opt/franz/Franz &'
alias pycharm='/opt/Pycharm/pycharm-2017.2.4/bin/pycharm.sh &'
alias venv='source ~/workspace/virtualenvs/venv/bin/activate'
alias delpyc='sudo find . \( -name __pycache__ -o -name "*.pyc" \) -delete'
